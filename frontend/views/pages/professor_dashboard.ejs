<%- include('./includes/head') %>

  <body>

    <%- include('./includes/navbar') %>

      <div class="container">

        <div class="row py-5 text-center">

          <div class="col-md-6">

            <i class="fas fa-square-root-alt" id="dashboard_icones"></i>
            <h2 class="my-2" id="dashboard_titulo">EXERCÍCIOS</h2>
            <p id="dashboard_paragrafo">encontre aqui todas as suas provas e exercícios</p>

          </div>

          <div class="col-md-6">

            <i class="far fa-clipboard" id="dashboard_icones"></i>
            <h2 class="my-2" id="dashboard_titulo">ANOTAÇÕES</h2>
            <p id="dashboard_paragrafo">...a gente sabe que às vezes dá aquele branco...</p>

          </div>

        </div>

        <div>
          <span style="border-radius: 0;" id="falso_btn_terciario">meus cursos</span>
        </div>

        <% if (disciplinas.length> 0) { %>
          <% disciplinas.forEach(disciplina=> { %>
            <a href="/disciplinas/<%= disciplina.id %>/topicos">

              <p class="my-3" id="dashboard_curso">
                - <%= disciplina.nome %>
              </p>

            </a>
            <% }) %>
              <% } %>

      </div>

  </body>

  </html>
